<!-- banner goes here!-->

<div class="header">
<%= image_tag 'contact_bg.jpg', :alt => 'contact_bg'%>

</div>

 <div class="container">

    <div class="sixteen columns">

<h1>Contact</h1>
<h5>We would love to hear from you</h5>

    </div>
   
<form id="form1" name="form1" class="wufoo contactInfoForm" accept-charset="UTF-8" autocomplete="off" enctype="multipart/form-data" method="post" novalidate
      action="https://gamelanrakyat.wufoo.com/forms/z108qnzv0a1dr2h/#public">
      
    
	<input id="Field3" class="formName field" type="text" placeholder="name" name="Field3">
	<input id="Field4" class="formEmail field" type="text" placeholder="email" name="Field4">
	<div class="purposeRadio">
		<input id="Field1_0" class="radioButton field" type="radio" name="purpose" value="workshop">Workshop<br>
		<input id="Field1_1" class="radioButton field" type="radio" name="purpose" value="funderSupporter">Funder/Supporter<br>
		<input id="Field1_2" class="radioButton field" type="radio" name="purpose" value="feedback">Feedback<br>
		<input id="Field1_3" class="radioButton field" type="radio" name="purpose" value="other">Other<br>
	</div>
	
	<textarea id="Field5" name="Field5" class="field dearGamelan formTextfield textarea" placeholder="Dear.Gamelan Rakyat..."></textarea>

	<input id="saveForm" name="saveForm" class="btTxt submit" type="submit" value"send">
	
</form>

 </div>

